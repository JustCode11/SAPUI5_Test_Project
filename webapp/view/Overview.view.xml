<mvc:View
	controllerName="sap.ui.test.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
>
	<Page title="{i18n>overviewPage}">
		<headerContent>
			<Button
				icon="sap-icon://write-new"
				text="Create Employee"
				press=".onOpenEmployeeForm"
			/>
			<Button
				text="Go to Large Table"
				press=".goToLargeTablePage"
			/>
			<Button
				text="Go to Test Page"
				press=".goToTestPage"
			/>
			<Button
				text="Go to IconTabBar Test"
				press=".goToIconTabBarTest"
			/>
			<Button
				text="Go to TableTest"
				press=".goToTableTest"
			/>
		</headerContent>
		<content>
			<!--<mvc:XMLView viewName="sap.ui.test.view.EmployeeList" />-->
			<mvc:XMLView viewName="sap.ui.test.view.EmployeeTable"/>
		</content>
	</Page>
</mvc:View>