<mvc:View
	controllerName="sap.ui.test.controller.IconTabBarTest"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
>
	<Page
		id="IconTabBarTestPage"
		title="Test Icon Tab Bar"
		backgroundDesign="Solid"
		navButtonTooltip="Go back"
		showNavButton="true"
		navButtonPress="goBack"
		titleAlignment="Center"
		titleLevel="H1"
	>
		<IconTabHeader
			id="iconTabHeader"
			items="{/Data}"
			class="sapUiMediumMarginBottom"
			selectedKey="01"
			select=".onSelect"
		>
			<items>
				<IconTabFilter
					key="{Id}"
					text="{CategoryName} ({NumberOfEntries})"
				/>
			</items>
		</IconTabHeader>
		<content>
			<!--<HBox
				width="100%"
				justifyContent="SpaceBetween"
				alignItems="End">
				<VBox>
				<Button text="Button 1" />
				<Button text="Button 2" />
				<Button text="Button 3" />
				</VBox>
				<VBox>
					<Label text="Label 1"></Label>
					<Input ></Input>
				</VBox>
			</HBox>-->
		</content>
	</Page>
</mvc:View>