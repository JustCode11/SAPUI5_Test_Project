{
    "_version": "1.12.0",
    "sap.app": {
        "id": "sap.ui.test",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "sap.ui.test.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.93",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.ui.test.i18n.i18n",
                    "supportedLocales": [
                        ""
                    ],
                    "fallbackLocale": ""
                }
            },
            "employee": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "Employees.json"
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "sap.ui.test.view",
                "controlId": "app",
                "controlAggregation": "pages"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "overview",
                    "target": "overview"
                },
                {
                    "pattern": "detail/{employeePath}",
                    "name": "detail",
                    "target": "detail"
                },
                {
                    "pattern": "employeeForm",
                    "name": "employeeForm",
                    "target": "employeeForm"
                }
            ],
            "targets": {
                "overview": {
                    "id": "overview",
                    "name": "Overview"
                },
                "detail": {
                    "id": "detail",
                    "name": "Detail"
                },
                "employeeForm": {
                    "id": "employeeForm",
                    "name": "EmployeeForm"
                }
            }
        }
    }
}